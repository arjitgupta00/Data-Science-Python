    "def Table(a):\n",
    "    i = 1\n",
    "    print(f\"\\nTable of {a}:\\n\")\n",
    "    while(i<=12):\n",
    "        print(f'{a} * {i} = {a*i}')\n",
    "        i += 1 \n",
    "    "
